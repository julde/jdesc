MultiTrackerHelpCenter{
		classvar <> helpStrings;
	*initClass{
		helpStrings = (
			\SymbolMultiTrackObject : "
SymbolMultiTrackObject :
	key down actions:
	?		: 	view this help file
	->		:	move object right on the timeline
	<-		:	move object left on the timeline
	arrowUp	:	move object one track up
	arrowDown:	move object one track down
	]		:	shorten the object's duration to the current time pointer
	[		:	move object to current time
	i		: 	get an info panel for this object
	e		:	get an editor
	a		: 	if in envelope mode then this adds a point to it
			
			
			
			
			"
		
		
		
		)
	
	
	}

}